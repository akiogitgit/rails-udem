<!-- ユーザー登録form -->
<%= form_with model: @user, local: true, class: "" do |f| %>
  <div class="form-group">
    <%= f.label :name, class: "" %>
    <%= f.text_field :name, class: "form-input" %>
  </div>
  <div class="form-group">
    <%= f.label :password, class: "" %>
    <%= f.password_field :password, class: "form-input" %>
  </div>
  <div class="form-group">
    <%= f.label :password_confirmation, class: "" %>
    <%= f.password_field :password_confirmation, class: "form-input" %>
  </div>
  <div class="form-group">
    <%= f.label :image, "プロフィール画像", class: "" %>
    <%= f.file_field :image %>
  </div>
  <%= f.submit "作成", class: "primary-btn float-right" %>
<% end %>
